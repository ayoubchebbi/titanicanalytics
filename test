function refreshViewVehicleStock(TerritoryId) {
    var divViewStock = document.getElementById('lightningvf');
    if (!divViewStock) {
        console.error("Div element 'lightningvf' not found.");
        return;
    }
    divViewStock.innerHTML = ""; // Clear previous content

    // Call the component
    $Lightning.use("c:FSCIT_includeViewVehicleStockonVFPage", function() {
        $Lightning.createComponent("c:prdcoViewVehiclesStock", { "recordId": TerritoryId }, "lightningvf", function(cmp, error) {
            if (error) {
                console.error("Error creating component:", error);
            } else {
                // do some stuff
            }
        });
    });
}
