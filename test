function formatTimeRange(schedStartTime, schedEndTime) {
    const startTime = new Date(schedStartTime).toLocaleTimeString('fr-FR', {
        hour: '2-digit',
        minute: '2-digit'
    });
    
    const endTime = new Date(schedEndTime).toLocaleTimeString('fr-FR', {
        hour: '2-digit',
        minute: '2-digit'
    });
    
    return `${startTime} - ${endTime}`;
}
